{
    "IpRateLimiting": {
        "EnableEndpointRateLimiting": true,
        "StackBlockedRequests": false,
        "RealIpHeader": "X-Real-IP",
        "ClientIdHeader": "X-ClientId",
        "HttpStatusCode": 429,
        "GeneralRules": [
            {
                "Endpoint": "*",
                "Period": "1m",
                "Limit": 100
            },
            {
                "Endpoint": "*",
                "Period": "1h",
                "Limit": 1000
            }
        ],
        "EndpointRules": [
            {
                "Endpoint": "POST:/api/users/login",
                "Period": "1m",
                "Limit": 5
            },
            {
                "Endpoint": "POST:/api/users/login",
                "Period": "15m",
                "Limit": 20
            }
        ]
    },
    "IpRateLimitPolicies": {
        "IpRules": [
            {
                "Ip": "127.0.0.1",
                "Rules": [
                    {
                        "Endpoint": "*",
                        "Period": "1s",
                        "Limit": 1000
                    }
                ]
            }
        ]
    }
}